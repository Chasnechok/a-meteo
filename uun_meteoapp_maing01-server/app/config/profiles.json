{
  "{asid}": {
    "profileList": ["AsidAuthorities","AsidExecutives","AsidLicenseOwner","Public"],  
    "useCaseMap": {}
  },
  "*": {
    "profileList": ["Authorities","Executives","AwidLicenseOwner","Public","Owner", "Employee","Sensor"],
    "useCaseMap": {
      "sys/uuAppWorkspace/init": {
        "sysStateList": ["created"],
        "profileList": ["AwidLicenseOwner"]
      },
      "defaultUve": {
        "profileList": ["Public"]
      },
      "sys/uuAppWorkspace/initUve": {
        "sysStateList": ["created"],
        "profileList": ["Public","AwidLicenseOwner"]
      },

      
      "sensor/create": {
        "sysStateList": ["active"],
        "profileList": ["Public"]
    },
      "sensor/get": {
        "sysStateList": ["active"],
        "profileList": ["Owner","Employee"]
      },
      "sensor/update": {
        "sysStateList": ["active"],
        "profileList": ["Owner"]
      },
      "sensor/list": {
        "sysStateList": ["active"],
        "profileList": ["Owner","Employee"]
      },
      "sensor/delete": {
        "sysStateList": ["active"],
        "profileList": ["Owner"]
      },





      "log/create": {
        "sysStateList": ["active"],
        "profileList": ["Owner","Sensor"]
      },
      "log/get": {
        "sysStateList": ["active"],
        "profileList": ["Owner", "Employee"]
      },
      "log/list": {
        "sysStateList": ["active"],
        "profileList": ["Owner", "Employee"]
      },
      "log/listBySensorCode": {
        "sysStateList": ["active"],
        "profileList": ["Owner", "Employee"]
      },
      "log/listByLocationCode": {
        "sysStateList": ["active"],
        "profileList": ["Owner", "Employee"]
      },
      "log/bulkCreate": {
        "sysStateList": ["active"],
        "profileList": ["Sensor"]
      },
      "log/delete": {
        "sysStateList": ["active"],
        "profileList": ["Owner"]
      },





      "location/create": {
        "sysStateList": ["active"],
        "profileList": ["Owner"]
      },
      "location/get": {
        "sysStateList": ["active"],
        "profileList": ["Owner", "Employee"]
      },
      "location/delete": {
        "sysStateList": ["active"],
        "profileList": ["Owner"]
      },
      "location/update": {
        "sysStateList": ["active"],
        "profileList": ["Owner"]
      },
      "location/list": {
        "sysStateList": ["active"],
        "profileList": ["Owner", "Employee"]
      }
      
    }
  }
}